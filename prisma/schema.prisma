generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model List {
  id        Int      @id @default(autoincrement())
  createdAt DateTime @default(now())
  channelId String
  users     User[]
}

model User {
  id     String   @id
  List   List? @relation(fields: [listId], references: [id])
  listId Int?
}
